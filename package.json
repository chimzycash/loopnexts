{
  "name": "rkstwb",
  "version": "1.0.0",
  "description": "Isomorphic React application boilerplate.",
  "repository": "git@github.com:miZyind/rkstwb.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "serve": "node dist/index.js",
    "start": "cross-env NODE_ENV=development webpack --config config/webpack/server/webpack.dev.ts && node dist/index.js",
    "analyze": "cross-env ANALYZE_BUNDLE=true yarn build",
    "analyze:server": "cross-env ANALYZE_BUNDLE=true yarn build:server",
    "analyze:client": "cross-env ANALYZE_BUNDLE=true yarn build:client",
    "build": "yarn clean && yarn build:server && yarn build:client",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack/server/webpack.prod.ts",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack/client/webpack.prod.ts"
  },
  "devDependencies": {
    "@types/autoprefixer": "^6.7.3",
    "@types/koa": "^2.0.44",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-static": "^4.0.0",
    "@types/node": "^9.4.6",
    "@types/pino": "^4.7.1",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/webpack": "^3.8.8",
    "@types/webpack-bundle-analyzer": "^2.9.1",
    "assets-webpack-plugin": "^3.5.1",
    "autodll-webpack-plugin": "^0.3.9",
    "autoprefixer": "^8.0.0",
    "compression-webpack-plugin": "^1.1.6",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "optimize-js-plugin": "^0.0.4",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.0",
    "react-dev-utils": "^5.0.0",
    "react-hot-loader": "^3.1.3",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "ts-loader": "^3.5.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.4.1",
    "koa-compose": "^4.0.0",
    "koa-logger": "^3.1.0",
    "koa-static": "^4.0.2",
    "koa-webpack-middleware": "^1.0.7",
    "pino": "^4.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-ui-css": "^2.2.14",
    "semantic-ui-react": "^0.78.2",
    "styled-components": "^3.1.6"
  }
}
