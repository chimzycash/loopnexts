{
  "name": "loopnexts",
  "version": "3.0.0",
  "description": "Isomorphic React scraffolding.",
  "repository": "git@github.com:miZyind/loopnexts.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register -T server/index.ts",
    "build:apidocs": "lb-apidocs",
    "clean": "lb-clean build",
    "lint": "yarn prettier:check && yarn tslint",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js`\"",
    "prettier:check": "yarn prettier:cli -- -l",
    "prettier:fix": "yarn prettier:cli -- --write",
    "tslint": "lb-tslint",
    "pretest": "npm run clean && npm run build",
    "posttest": "npm run lint",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "babel": {
    "presets": [
      "next/babel",
      "@zeit/next-typescript/babel"
    ],
    "plugins": [
      "styled-components"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@loopback/boot": "^1.0.6",
    "@loopback/context": "^1.2.0",
    "@loopback/core": "^1.1.1",
    "@loopback/openapi-v3": "^1.1.3",
    "@loopback/repository": "^1.0.6",
    "@loopback/rest": "^1.4.0",
    "@loopback/rest-explorer": "^1.1.2",
    "@loopback/service-proxy": "^1.0.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "chalk": "^2.4.1",
    "dotenv": "^6.2.0",
    "next": "^7.0.2",
    "next-optimized-images": "^2.3.1",
    "next-routes": "^1.4.2",
    "pino": "^5.10.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@loopback/build": "^1.0.1",
    "@loopback/testlab": "^1.0.1",
    "@types/next": "^7.0.6",
    "@types/node": "^10.11.2",
    "@types/pino": "^5.8.3",
    "@types/styled-components": "^4.1.4",
    "@types/webpack": "^4.4.22",
    "babel-plugin-styled-components": "^1.10.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint-react": "^3.6.0"
  }
}
